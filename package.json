{
  "name": "boh_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -c auto npm:dev:generate npm:dev:api npm:dev:vite",
    "dev:generate": "yarn run generate:schema -w && yarn run generate:query -w",
    "dev:api": "hatch run server",
    "dev:vite": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate": "yarn run generate:schema && yarn run generate:query",
    "generate:schema": "mkdir -p src/front/gql && hatch run schema src/front/gql/schema.graphql",
    "generate:query": "graphql-codegen --config=.graphqlrc.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mui/material": "^5.14.12",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "urql": "^4.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@parcel/watcher": "^2.3.0",
    "@types/node": "^20.8.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.1.0",
    "@yarnpkg/sdks": "*",
    "concurrently": "^8.2.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  },
  "packageManager": "yarn@3.6.4"
}
