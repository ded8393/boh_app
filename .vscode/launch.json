  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["boh_app.server:app", "--reload"],
      "jinja": true,
      "console": "internalConsole",
      "justMyCode": false,
      "env": {
        "DEBUG": "1",
      },
    },
    {
      "name": "Python: Generate all",
      "type": "python",
      "request": "launch",
      "module": "boh_app",
      "args": ["gen-all"],
      "console": "internalConsole",
      "justMyCode": false,
    },
    {
      "name": "Python: Test",
      "type": "python",
      "purpose": ["debug-test"],
      "request": "launch",
      "program": "${file}",
      "console": "internalConsole",
      "justMyCode": false,
    },
    {
      "name": "Python: Debug File",
      "type": "python",
      "purpose": ["debug-in-terminal"],
      "request": "launch",
      "program": "${file}",
      "console": "internalConsole",
      "justMyCode": false,
    },
  ],
}
